{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/wire/main_multivariate.py",
            "cwd":"${workspaceFolder}",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"},
            "justMyCode": false
        },
        {
            "name": "Profile",
            "type": "python",
            "request": "launch",
            "module": "cProfile",
            "args": [
                "-o", "/tmp/tmp.prof", "${workspaceFolder}/wire/main_multivariate.py"
            ],
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"},
            "justMyCode": false
        }
    ]
}